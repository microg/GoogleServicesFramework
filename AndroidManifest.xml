    <application
        android:icon="@drawable/app_icon"
        android:label="@string/app_name" >
        <provider
            android:name=".gservices.GservicesProvider"
            android:authorities="com.google.android.gsf.gservices" />

        <service
            android:name=".gtalkservice.PushMessagingRegistrarProxy"
            android:exported="true"
            android:permission="com.google.android.c2dm.permission.RECEIVE" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.REGISTER" />
            </intent-filter>
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.UNREGISTER" />
            </intent-filter>
        </service>

        <service
            android:name=".gtalkservice.PushMessagingRegistrar"
            android:permission="com.google.android.c2dm.permission.RECEIVE" />

        <receiver
            android:name=".gtalkservice.ReceiverProxy"
            android:permission="com.google.android.gsf.permission.C2D_MESSAGE" />

        <service
            android:name=".gtalkservice.service.GTalkService"
            android:exported="true" >
            <intent-filter>
                <action android:name="com.google.android.gtalkservice.IGTalkService" />
            </intent-filter>
        </service>

        <provider
            android:name=".talk.TalkProvider"
            android:authorities="com.google.android.providers.talk"
            android:grantUriPermissions="true"
            android:multiprocess="false"
            android:readPermission="com.google.android.providers.talk.permission.READ_ONLY"
            android:writePermission="com.google.android.providers.talk.permission.WRITE_ONLY" >
            <path-permission
                android:path="/contacts/search_suggest_query"
                android:readPermission="android.permission.GLOBAL_SEARCH" />
        </provider>

        <receiver
            android:name=".gservices.GservicesProvider$OverrideReceiver"
            android:permission="com.google.android.providers.gsf.permission.WRITE_GSERVICES" >
            <intent-filter>
                <action android:name="com.google.gservices.intent.action.GSERVICES_OVERRIDE" />
            </intent-filter>
        </receiver>

        <service android:name=".checkin.CheckinService" />

        <receiver android:name=".checkin.CheckinService$Receiver" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <action android:name="android.intent.action.TIME_SET" />
                <action android:name="android.net.conn.BACKGROUND_DATA_SETTING_CHANGED" />
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
                <action android:name="com.google.gservices.intent.action.GSERVICES_CHANGED" />
            </intent-filter>
        </receiver>
        <receiver android:name=".checkin.CheckinService$TriggerReceiver" >
            <intent-filter>
                <action android:name="android.server.checkin.CHECKIN" />
            </intent-filter>
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />

                <category android:name="android.server.checkin.CHECKIN" />
            </intent-filter>
        </receiver>
        <receiver android:name=".checkin.CheckinService$SecretCodeReceiver" >
            <intent-filter>
                <action android:name="android.provider.Telephony.SECRET_CODE" />

                <data
                    android:host="2432546"
                    android:scheme="android_secret_code" />
            </intent-filter>
        </receiver>

        <service android:name=".checkin.EventLogService" />

        <receiver android:name=".checkin.EventLogService$Receiver" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <action android:name="android.intent.action.TIME_SET" />
                <action android:name="com.google.gservices.intent.action.GSERVICES_CHANGED" />
            </intent-filter>
        </receiver>

        <provider
            android:name=".subscribedfeeds.SubscribedFeedsProvider"
            android:authorities="subscribedfeeds"
            android:exported="true"
            android:multiprocess="false"
            android:readPermission="android.permission.SUBSCRIBED_FEEDS_READ"
            android:writePermission="android.permission.SUBSCRIBED_FEEDS_WRITE" />

        <receiver
            android:name=".subscribedfeeds.SubscribedFeedsGCMBroadcastReceiver"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <action android:name="com.google.android.c2dm.intent.REGISTRATION" />

                <category android:name="com.google.android.gsf.subscribedfeeds" />
